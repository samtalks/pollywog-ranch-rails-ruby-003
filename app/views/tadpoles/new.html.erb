<h1>Create a new tadpole for <%= @frog.name %></h1>
<form action="/tadpoles" method="post">
  <label for="name">Name</label>
  <input type="text" id="name" name="name">
  <input type="hidden" id="color" name="color" value="<%= @frog.color %>">
  <input type="hidden" name="frog_id" value="<%= @frog.id %>">
  <input type="submit" value="create">
</form><br>
<a href="/tadpoles">Cancel</a>